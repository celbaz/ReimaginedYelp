<% if (userId === currentUser) { %>
  <div id="addplace">
    Create A Restaurant
  </div>
  <hr>
<% } %>

  <h3 class="restur">Manage Restaurants</h3>

<% places.each( function(place){ %>
  <div class="rest-wrapper group">
      <div class ="manage-rest">
        <% for( i in place.attributes){ %>
          <% if (i !== "user_id" && i !== "id") { %>
            <p><%= i %> : <%= place.escape(i) %></p>
          <% } %>
          <% } %>
      </div>
      <% if (userId === currentUser) { %>

      <button class="rest-edit edit-clicked" data-id="<%= place.escape("id") %>" name="edit">Edit</button>
      <button class="delete-rest rest-edit" data-id="<%= place.escape("id") %>"
        name="Delete">Delete</button>
      <% } %>

  </div>

  <% }); %>
